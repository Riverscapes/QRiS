                    <LookupValues>
                        <LookupValue>Bowl</LookupValue>
                        <LookupValue>Mound</LookupValue>
                        <LookupValue>Plane</LookupValue>
                        <LookupValue>Saddle</LookupValue>
                        <LookupValue>Trough</LookupValue>
                    </LookupValues>
                </Field>
            </Fields>
        </Layer>
        <Layer machine_code="geomorphic_units" version="1.0" geom_type="Point">
            <Label>Geomorphic Unit Points</Label>
