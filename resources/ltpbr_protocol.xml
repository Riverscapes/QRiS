<Protocol machine_code="LTPBR_BASE" version="1.0" status="experimental">
    <Label>LTPBR Protocol</Label>
    <Description>Base LTPBR Layers</Description>
    <URL>https://lowtechpbr.restoration.usu.edu/</URL>
    <Citation> Wheaton J.M., Bennett S.N., Bouwes, N., Maestas J.D. and Shahverdian S.M. (Editors). 2019. Low-Tech Process-Based Restoration of Riverscapes: Design Manual. Version 1.0. Utah State University Restoration Consortium. Logan, UT. 286 pp. DOI: 10.13140/RG.2.2.19590.63049/2.</Citation>
    <Author>Nick Weber</Author>
    <CreationDate>2024-09-01</CreationDate>
    <UpdatedDate>2024-09-01</UpdatedDate>
    <Metadata>
        <MetadataItem>
            <Name>My Meta</Name>
            <Value>Data</Value>
        </MetadataItem>
    </Metadata>
    <Layers>
        <Layer machineCode="dam_crests" version="1.0" geom_type="Linestring">
            <Label>Dam Crests</Label>
            <Symbology>beaver_dams.qml</Symbology>
            <Description>Dam Crests Layer</Description>
            <Hierarchy>
                <HierarchyItem>Assessments</HierarchyItem>
                <HierarchyItem>Beaver Dam Building</HierarchyItem>
            </Hierarchy>
            <Fields>
                <Field machineCode="structure_source_id" version="1.0" type="list">
                    <Label>Structure Source</Label>
                    <LookupValues>
                        <LookupValue>Artificial</LookupValue>
                        <LookupValue>Natural</LookupValue>
                        <LookupValue>Unknown</LookupValue>
                    </LookupValues>
                </Field>
                <Field machine_code="dam_state" version="1.0" type="list">
                    <Label>Dam State</Label>
                    <LookupValues>
                        <LookupValue>Blown Out</LookupValue>
                        <LookupValue>Breached</LookupValue>
                        <LookupValue>Intact</LookupValue>
                    </LookupValues>              
                </Field>
                <Field machine_code="crest_type" version="1.0" type="list">
                    <Label>Crest Type</Label>
                    <LookupValues>
                        <LookupValue>Active</LookupValue>
                        <LookupValue>Inactive</LookupValue>
                    </LookupValues>
                </Field>
                <Field machine_code="confidence" version="1.0" type="list">
                    <Label>Confidence</Label>
                    <LookupValues>
                        <LookupValue>High</LookupValue>
                        <LookupValue>Medium</LookupValue>
                        <LookupValue>Low</LookupValue>
                    </LookupValues>
                </Field>
        <Layer machineCode="active_channel" version="1.0" geom_type="Polygon">
            <Label>Active Channel</Label>
            <Symbology>active_channels.qml</Symbology>
            <Description>Active Channel Layer</Description>
            <Hierarchy>
                <HierarchyItem>Observations</HierarchyItem>
                <HierarchyItem>Geomorphic Mapping</HierarchyItem>
                <HierarchyItem>Channel</HierarchyItem>
            </Hierarchy>
        </Layer>
        <Layer machineCode="active_extents" version="1.0" geom_type="Polygon">
            <Label>Active Extents</Label>
            <Symbology>active_extents.qml</Symbology>
            <Description>Active Extents Layer</Description>
            <MenuItems>
                <MenuItem>Copy_From_Valley_Bottom</MenuItem>
            </MenuItems>
            <Hierarchy>
                <HierarchyItem>Observations</HierarchyItem>
                <HierarchyItem>Geomorphic Mapping</HierarchyItem>
                <HierarchyItem>Floodplain</HierarchyItem>
            </Hierarchy>
            <Fields>
                <Field machineCode="type_id" version="1.0" type="list">
                    <Label>Type</Label>
                    <LookupValues>
                        <LookupValue>Active</LookupValue>
                        <LookupValue>Inactive</LookupValue>
                    </LookupValues>
                </Field>
            </Fields>
        </Layer>
        <Layer machineCode="centerlines" version="1.0" geom_type="Linestring">
            <Label>Active Channel Lines</Label>
            <Symbology>centerlines.qml</Symbology>
            <Description>Centerlines of the Active Channel</Description>
            <Hierarchy>
                <HierarchyItem>Observations</HierarchyItem>
                <HierarchyItem>Geomorphic Mapping</HierarchyItem>
                <HierarchyItem>Channel</HierarchyItem>
            </Hierarchy>
            <Fields>
                <Field machineCode="type_id" version="1.0" type="list">
                    <Label>Type</Label>
                    <LookupValues>
                        <LookupValue>Primary</LookupValue>
                        <LookupValue>Non-Primary</LookupValue>
                    </LookupValues>
                </Field>
                <Field machineCode="description" version="1.0" type="list">
                    <Label>Description</Label>
                    <DefaultValue>Centerline</DefaultValue>
                    <LookupValues>
                        <LookupValue>Centerline</LookupValue>
                        <LookupValue>Thalweg</LookupValue>
                    </LookupValues>
                </Field>    
            </Fields>
        </Layer>
        <Layer machineCode="inundation_extents" version="1.0" geom_type="Polygon">
            <Label>Inundation Extents</Label>
            <Symbology>inundation_extents.qml</Symbology>
            <Description>Inundation Extents Layer</Description>
            <Hierarchy>
                <HierarchyItem>Observations</HierarchyItem>
                <HierarchyItem>Hydraulic Mapping</HierarchyItem>
            </Hierarchy>    
            <Fields>
                <Field machineCode="type_id" version="1.0" type="list">
                    <Label>Extent Type</Label>
                    <LookupValues>
                        <LookupValue>Free Flowing</LookupValue>
                        <LookupValue>Overflow</LookupValue>
                        <LookupValue>Ponded</LookupValue>
                    </LookupValues>
                </Field>
            </Fields>
        </Layer>
        <Layer machine_code="channel_junctions" version="1.0" geom_type="Point">
