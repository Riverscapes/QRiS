                <Field machine_code="width" version="1.0" type="float">
                    <Label>Width</Label>
                </Field>
                <Field machine_code="depth" version="1.0" type="float">
                    <Label>Depth</Label>
                </Field>
            </Fields>
        </Layer>
        "fields": [{"machine_code": "sem_stage", "label": "SEM Stage", "type": "list", "lookup": "sem_stages"}]
        <Layer machineCode="cem_phases" version="1.0" geom_type="Polygon">
            <Label>SEM Cluer &amp; Thorne</Label>
            <Symbology>cem_phases.qml</Symbology>
            <Description>SEM Cluer &amp; Thorne Layer</Description>
            <Hierarchy>
                <HierarchyItem>Assessments</HierarchyItem>
                <HierarchyItem>Geomorphic Condition</HierarchyItem>
            </Hierarchy>
            <MenuItems>
                <MenuItem>Copy_From_Valley_Bottom</MenuItem>
            </MenuItems>
            <Fields>
                <Field machineCode="sem_stage" version="1.0" type="list">
                    <Label>SEM Stage</Label>
                    <LookupValues>
                        <LookupValue>0 Anastomosing</LookupValue>
                        <LookupValue>1 Sinuous Single Thread</LookupValue>
                        <LookupValue>2 Channelized</LookupValue>
                        <LookupValue>3 Degradation</LookupValue>
                        <LookupValue>4 Degradation and Widening</LookupValue>
                        <LookupValue>5 Aggradation and Widening</LookupValue>
                        <LookupValue>6 Quasi Equilibrium</LookupValue>
                        <LookupValue>7 Laterally Active</LookupValue>
                        <LookupValue>8 Anastomosing</LookupValue>
                    </LookupValues>
                </Field>
            </Fields>
        </Layer>
