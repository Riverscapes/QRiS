                        <LookupValue>Mid Channel Bar</LookupValue>
                        <LookupValue>Pond</LookupValue>
                        <LookupValue>Pool</LookupValue>
                        <LookupValue>Rapid</LookupValue>
                        <LookupValue>Riffle</LookupValue>
                    </LookupValues>
                </Field>
                <Field machine_code="geomorphic_unit_type_2_tier" version="1.0" type="list">
                    <Label>Type 2 Tier</Label>
                    <LookupValues>
                        <LookupValue>Bowl</LookupValue>
                        <LookupValue>Mound</LookupValue>
                        <LookupValue>Plane</LookupValue>
                        <LookupValue>Saddle</LookupValue>
                        <LookupValue>Trough</LookupValue>
                    </LookupValues>
                </Field>
